            <div class="page-container">


                <!-- Content Wrapper START -->
                <div class="main-content">
                    @RenderBody()                
                    @if (ViewBag.TotalPage != null && ViewBag.TotalPage > 1)
                    {
                        var limit = ViewBag.Limit != null ? ViewBag.Limit : 10;
                        <nav>
                            <ul class="pagination">
                                <li class="page-item @(ViewBag.CurrentPage==1?"disabled":"")">
                                    <a class="page-link" href="@Url.Action(_action,_controller, new {page = ViewBag.CurrentPage-1, limit=limit})" tabindex="-1">Previous</a>
                                </li>
                                @for (int i = 1; i <= ViewBag.TotalPage; i++)
                                {
                                    <li class="page-item @(ViewBag.CurrentPage==i?"active":"")">
                                        <a class="page-link" href="@Url.Action(_action,_controller, new {page = i, limit=limit})">@i</a>
                                    </li>
                                }
                                <li class="page-item @(ViewBag.CurrentPage==ViewBag.TotalPage?"disabled":"")">
                                    <a class="page-link" href="@Url.Action(_action,_controller, new {page = ViewBag.CurrentPage+1, limit=limit})">Next</a>
                                </li>
                            </ul>
                        </nav>
